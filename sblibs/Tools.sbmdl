module STD_TOOLS;

requires: STD;

#define STD_TOOLS

#define UNO(_X) @TW.Where(w => w.Start == "_X".LastChar).Where(w => (w.Name.Length > 6 ? "_X".Length > 6 : w.Name.Length == "_X".Length) || w.\c(/_X/d.Type1) || (/X/d.Type2 != $I && w.Contains(/_X/d.Type2))).Sort()

#define STARTS(_X) @TW.Where(Start == '_X')
#define ENDS(_X) @TW.Where(`_Xー*$`.\~(Name))
#define STARTSN(_X) @NN.Where(FirstChar == '_X')
#define ENDSN(_X) @NN.Where(`_Xー*$`.\~(@item))